{
  "name": "dastrong-portfolio",
  "author": "Daniel Strong",
  "version": "1.0.0",
  "description": "Daniel Strong's Portfolio",
  "license": "UNLICENSED",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "postbuild": "next-sitemap --config next-sitemap.config.js",
    "start": "next start",
    "analyze": "cross-env ANALYZE=true next build",
    "prepare": "husky install",
    "format": "prettier --write components/**/* pages/**/* utils/**/* hooks/**/*",
    "check-format": "prettier --check components/**/* pages/**/* utils/**/* hooks/**/*",
    "check-lint": "eslint . --ext ts --ext tsx --ext js",
    "check-types": "tsc --pretty --noEmit",
    "lighthouse-setup": "lighthouse-batch -o ./reports/lighthouse --no-report -h",
    "check-lighthouse-mobile": "yarn lighthouse-setup -p \"--view\"",
    "check-lighthouse-desktop": "yarn lighthouse-setup -p \"--view --preset=desktop\""
  },
  "dependencies": {
    "@plaiceholder/next": "^2.2.0",
    "chrome-aws-lambda": "7.0.0",
    "clsx": "^1.1.1",
    "focus-visible": "^5.2.0",
    "next": "^12.1.0",
    "plaiceholder": "^2.2.0",
    "probe-image-size": "^7.2.2",
    "puppeteer-core": "7.0.0",
    "react": "^17.0.2",
    "react-copy-to-clipboard": "^5.0.4",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-intersection-observer": "^8.32.1",
    "react-markdown": "^7.1.0",
    "react-share": "^4.4.0",
    "react-syntax-highlighter": "^15.4.4",
    "rehype-autolink-headings": "^6.1.0",
    "rehype-slug": "^5.0.0",
    "remark-gfm": "^3.0.1",
    "sharp": "^0.29.3",
    "styled-components": "^5.3.3",
    "use-ackee": "^3.0.1"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^12.0.7",
    "@next/eslint-plugin-next": "^12.0.7",
    "@types/node": "^16.11.4",
    "@types/puppeteer-core": "^5.4.0",
    "@types/react": "^17.0.31",
    "@types/react-copy-to-clipboard": "^5.0.2",
    "@types/react-syntax-highlighter": "^13.5.2",
    "@types/styled-components": "^5.1.15",
    "@types/use-ackee": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "gray-matter": "^4.0.3",
    "husky": "^7.0.4",
    "lighthouse-batch": "^8.1.1",
    "lint-staged": "^11.2.3",
    "next-sitemap": "^1.6.192",
    "prettier": "2.4.1",
    "raw-loader": "^4.0.2",
    "typescript": "^4.4.4"
  },
  "lint-staged": {
    "*.{js,ts,tsx,json}": [
      "eslint --fix"
    ]
  }
}
